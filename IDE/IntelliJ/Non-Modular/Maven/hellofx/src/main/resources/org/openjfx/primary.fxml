<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<FlowPane alignment="CENTER" orientation="VERTICAL" xmlns="http://javafx.com/javafx/17.0.2-ea"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryController">
    <FlowPane fx:id="panel1" alignment="TOP_CENTER" hgap="10" prefWidth="200">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Category List" textAlignment="CENTER">
            <font>
                <Font size="26.0"/>
            </font>
        </Text>
        <ListView fx:id="CategoryListView" prefHeight="400" prefWidth="200.0">
            <FlowPane.margin>
                <Insets top="8.0"/>
            </FlowPane.margin>
        </ListView>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </FlowPane>
    <FlowPane fx:id="panel2" alignment="TOP_CENTER" hgap="10" prefWidth="200">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Project List" textAlignment="CENTER">
            <font>
                <Font size="26.0"/>
            </font>
        </Text>
        <ListView fx:id="ProjectListView" prefHeight="400" prefWidth="200.0">
            <FlowPane.margin>
                <Insets top="8.0"/>
            </FlowPane.margin>
        </ListView>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </FlowPane>
    <FlowPane fx:id="panel3" alignment="TOP_CENTER" hgap="10" prefWidth="200">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Test List" textAlignment="CENTER">
            <font>
                <Font size="26.0"/>
            </font>
        </Text>
        <ListView fx:id="TestListView" prefHeight="400" prefWidth="200.0">
            <FlowPane.margin>
                <Insets top="8.0"/>
            </FlowPane.margin>
        </ListView>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </FlowPane>


    <FlowPane fx:id="panel4" alignment="TOP_CENTER" prefWidth="300">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Test Detail" textAlignment="CENTER">
            <font>
                <Font size="26.0"/>
            </font>
        </Text>
        <FlowPane fx:id="detailcontentpanel" prefHeight="400" prefWidth="300">
            <FlowPane.margin>
                <Insets top="8.0"/>
            </FlowPane.margin>
        </FlowPane>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </FlowPane>
</FlowPane>
